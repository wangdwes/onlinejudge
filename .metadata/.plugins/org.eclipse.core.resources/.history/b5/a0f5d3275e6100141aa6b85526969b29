
public class Solution {

    public void flatten(TreeNode root) {
    	
    }
    
    public void preOrderTraversal(TreeNode node) {
    	if (node == null) return;
    	preOrderTraversal(node.left);
    	preOrderTraversal(node.right);
    	
    	if (node.left != null) {
    		TreeNode leaf = node.left;
    		while (leaf.right != null)
    			leaf = leaf.right;
    	}
    	
    	
    }
	
}
