
public class Solution {
    public void merge(int A[], int m, int B[], int n) {
    	int aPointer = 0, bPointer = 0;
    	while (aPointer < m && bPointer < n) {
    		if (A[aPointer + bPointer] > B[bPointer]) {
    			for (int index = m + n - 1; index >= aPointer + bPointer; index--)
    				A[index] = A[index - 1];
    			A[aPointer + bPointer] = B[bPointer]; bPointer--; }
    		else aPointer++;
    	}
    }
}
