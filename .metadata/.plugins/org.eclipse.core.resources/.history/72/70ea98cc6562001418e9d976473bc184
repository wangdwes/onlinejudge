
public class Solution {

	// 1 2 3 4 5 6 7 8 9
	// 1 2 3 5 4 6 7 8 9: 5,4;
	// 1 2 6 4 5 3 7 8 9: 6,4;5,3;

	public TreeNode first;
	public TreeNode second;
	
	public void traversal(TreeNode node, int val) {
		if (node == null) return;
		traversal(node.left, node.val);
		if (node.val > val) { first = node; 
		traversal(node.right, node.val);
	}
	
    public void recoverTree(TreeNode root) {
        traversal(root);
    }
    
}
